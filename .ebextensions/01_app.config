option_settings:
  aws:elasticbeanstalk:container:java:
    JVMOptions: '-Xmx512m'
  aws:elasticbeanstalk:application:environment:
    SERVER_PORT: 5000
  aws:elb:loadbalancer:
    CrossZone: true
  aws:elb:policies:
    ConnectionDrainingEnabled: true
    ConnectionDrainingTimeout: 20
  aws:elb:healthcheck:
    HealthyThreshold: 2
    Interval: 10
    Timeout: 5
    UnhealthyThreshold: 10